<h1>Editing User</h1>

<% form_for(:user, @user, :url=>admin_user_path(@user), :html=>{:method=>'put'}) do |form| %>
  <%= render :partial => 'form', :locals => {:form => form} %>
  <p><%= form.submit 'Update' %></p>
<% end %>

<p>
  <%= link_to 'Show', admin_user_path(@user) %>
  |
  <%= link_to 'Back', admin_users_path %>
</p>
